{
    "tasks": [
		{
			"type": "shell",
			"label": "gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "/usr/bin"
			}
		},
		{
			"type": "opencl",
			"label": "opencl: custom build [sets] {gpu_64}",
			"command": "/System/Library/Frameworks/OpenCL.framework/Libraries/openclc",
			"args": [
				"-emit-llvm",
				"-c",
				"-arch",
				"gpu_64",
				"/Users/mburl/Desktop/Fractol/srcs/sets.cl",
				"-o sets.gpu_64.bc",
			],
			"problemMatcher": [
				"$opencl.common",
				"$opencl.openclc"
			]
		}
	],
    "version": "2.0.0"
}